{
  "name": "whs-server",
  "version": "3.0.0",
  "private": true,
  "types": "index.d.ts",
  "scripts": {
    "start": "node dist/backend/server.js",
    "serve": "vue-cli-service serve",
    "build:frontend": "vue-cli-service build --dest dist/frontend",
    "build:backend": "yarn tsc -p tsconfig.backend.json && rm -rf dist/shared && rm -rf dist/backend/types",
    "build": "yarn build:frontend && yarn build:backend",
    "lint:frontend": "vue-cli-service lint",
    "lint:backend": "yarn tslint -p tsconfig.backend.json",
    "lint": "yarn lint:frontend && yarn lint:backend",
    "test:unit": "vue-cli-service test:unit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "date-fns": "^1.30.1",
    "element-ui": "^2.4.5",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.2.7",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@shelf/jest-mongodb": "^1.0.1",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/helmet": "^0.0.43",
    "@types/jest": "^23.1.4",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mongodb": "^3.1.28",
    "@types/supertest": "^2.0.7",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-typescript": "^3.8.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^23.0.0",
    "typescript": "^3.4.3",
    "vue-template-compiler": "^2.6.10"
  }
}
